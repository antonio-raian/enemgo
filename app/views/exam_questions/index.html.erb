<p id="notice"><%= notice %></p>

<h1>Exam Questions</h1>

<table>
  <thead>
    <tr>
      <th>Exam</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exam_questions.each do |exam_question| %>
      <tr>
        <td><%= exam_question.exam %></td>
        <td><%= exam_question.question %></td>
        <td><%= link_to 'Show', exam_question %></td>
        <td><%= link_to 'Edit', edit_exam_question_path(exam_question) %></td>
        <td><%= link_to 'Destroy', exam_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exam Question', new_exam_question_path %>
